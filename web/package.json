{
  "name": "shopify-app-template-node",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "debug": "node --inspect-brk index.js",
    "dev": "concurrently \"npx tsc --watch --preserveWatchOutput --project tsconfig.json\" \"npx tsc-alias -w -p tsconfig.json\" \"cross-env NODE_ENV=development nodemon dist/index.js --delay 1\"",
    "preserve": "concurrently \"npx tsc --preserveWatchOutput --project tsconfig.json\" \"npx tsc-alias -p tsconfig.json\"",
    "serve": "cross-env NODE_ENV=production TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/index.js",
    "test": "jest --watch --runInBand --detectOpenHandles"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "dependencies": {
    "@shopify/shopify-api": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "express": "^4.17.3",
    "express-validator": "^6.14.2",
    "mongoose": "^6.6.5",
    "multer": "^1.4.5-lts.1",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@shelf/jest-mongodb": "^4.1.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "concurrently": "^7.5.0",
    "jest": "^29.2.2",
    "jest-environment-node": "^29.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^8.9.5",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
